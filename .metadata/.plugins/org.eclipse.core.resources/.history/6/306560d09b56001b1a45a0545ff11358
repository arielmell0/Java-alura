
public class TestaCaracteres {
	
	public static void main(String[] args) {
		char letra = 'a';
		System.out.println(letra);
		
		String frase = "A declaração String deve começar com letra maiuscula ";
		String frase2 = "A String deve conter aspas duplas e não simples";
		
		System.out.println(frase+frase2);
	}
}
